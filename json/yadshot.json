{
    "buildinfo": [
    {
        "prerun": [
            "curl -sSL -o ~/.cache/deb2appimage/debs/yad.deb 'https://launchpad.net/~webupd8team/+archive/ubuntu/y-ppa-manager/+files/yad_0.39.0-1~webupd8~trusty0_amd64.deb'",
            "mkdir -p ~/.cache/deb2appimage/AppDir/usr/bin",
            "cp ~/github/yadshot/yadshot.sh ~/.cache/deb2appimage/AppDir/usr/bin/yadshot.sh",
            "cp ~/github/yadshot/yadshot.desktop ~/.cache/deb2appimage/AppDir/usr/bin/yadshot.desktop",
            "cp ~/github/yadshot/yadshot.png ~/.cache/deb2appimage/AppDir/usr/bin/yadshot.png",
            "cp ~/bin/imagemagick ~/.cache/deb2appimage/AppDir/usr/bin/ImageMagick",
            "chmod a+x ~/.cache/deb2appimage/AppDir/usr/bin/yadshot.sh",
            "chmod a+x ~/.cache/deb2appimage/AppDir/usr/bin/ImageMagick"
        ],
        "name": "yadshot",
        "version": "0.1.95",
        "deps": "slop,libglew2.0,libicu57,libcairo2,libpango-1.0-0,libpangocairo-1.0-0,libsoup2.4-1,libwebkitgtk-3.0-0,libpng12-0,libjavascriptcoregtk-3.0-0,libgeoclue0,libicu52,libwebp5,libpangoft2-1.0-0",
        "repoarch": "amd64",
        "distrorepo": "Ubuntu",
        "repoversion": "artful,artful,artful,trusty",
        "binarypath": "/usr/bin/yadshot.sh",
        "desktoppath": "/usr/bin/yadshot.desktop",
        "iconpath": "/usr/bin/yadshot.png",
        "usewrapper": "true",
        "postrun": [
            null
        ]
    }
    ],
    "apprunconf": [
    {
        "setpath": "true",
        "setlibpath": "true",
        "setpythonpath": "false",
        "setpythonhome": "false",
        "setpythondontwritebytecode": "false",
        "setxdgdatadirs": "false",
        "setperllib": "false",
        "setgsettingsschemadir": "false",
        "setqtpluginpath": "false",
        "exec": "/usr/bin/yadshot.sh.wrapper"
    }
    ],
    "authors": [
    {
        "type": "Author",
        "author": "YAD",
        "url": "YAD"
    },
    {
        "type": "AppImage Maintainer",
        "author": "simonizor",
        "url": "http://www.simonizor.net"
    }
    ]
}
